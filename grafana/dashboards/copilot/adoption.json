        {
          "datasource": "mysql",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH bounds AS (\n  SELECT\n    $__timeFrom() AS from_time,\n    $__timeTo() AS to_time,\n    TIMESTAMPDIFF(SECOND, $__timeFrom(), $__timeTo()) AS span_sec\n)\nSELECT\n  bounds.from_time AS time,\n  COALESCE(\n    (\n      SELECT SUM(total_engaged_users)\n      FROM _tool_copilot_org_metrics\n      WHERE\n        date >= DATE_SUB(bounds.from_time, INTERVAL bounds.span_sec SECOND)\n        AND date < bounds.from_time\n        AND connection_id = ${connection_id}\n        AND scope_id = '${scope_id}'\n    ) / NULLIF((\n      SELECT SUM(total_active_users)\n      FROM _tool_copilot_org_metrics\n      WHERE\n        date >= DATE_SUB(bounds.from_time, INTERVAL bounds.span_sec SECOND)\n        AND date < bounds.from_time\n        AND connection_id = ${connection_id}\n        AND scope_id = '${scope_id}'\n    ), 0),\n    0\n  ) AS value\nFROM bounds\nUNION ALL\nSELECT\n  bounds.to_time AS time,\n  COALESCE(\n    (\n      SELECT SUM(total_engaged_users)\n      FROM _tool_copilot_org_metrics\n      WHERE\n        date >= bounds.from_time\n        AND date <= bounds.to_time\n        AND connection_id = ${connection_id}\n        AND scope_id = '${scope_id}'\n    ) / NULLIF((\n      SELECT SUM(total_active_users)\n      FROM _tool_copilot_org_metrics\n      WHERE\n        date >= bounds.from_time\n        AND date <= bounds.to_time\n        AND connection_id = ${connection_id}\n        AND scope_id = '${scope_id}'\n    ), 0),\n    0\n  ) AS value\nFROM bounds\nORDER BY time\n",
          "refId": "A"
        }
      ],
      "title": "Engaged Users %",
      "type": "stat"
