        {
          "datasource": "mysql",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH editor_sums AS (\n  SELECT\n    editor,\n    SUM(suggestions) AS suggestions\n  FROM _tool_copilot_language_metrics\n  WHERE\n    $__timeFilter(date)\n    AND connection_id = ${connection_id}\n    AND scope_id = '${scope_id}'\n  GROUP BY editor\n), total AS (\n  SELECT SUM(suggestions) AS total_suggestions\n  FROM editor_sums\n)\nSELECT\n  NOW() as time,\n  editor_sums.editor as metric,\n  COALESCE(editor_sums.suggestions / NULLIF(total.total_suggestions, 0), 0) as value\nFROM editor_sums\nCROSS JOIN total\nORDER BY editor_sums.suggestions DESC\n",
          "refId": "A"
        }
      ],
